package com.mc.lang.object;

public class Book {
	
	private String title;
	private Author author;
	private int price;
	
	public Book() {
		// TODO Auto-generated constructor stub
	}

	public Book(String title, Author author, int price) {
		super();
		this.title = title;
		this.author = author;
		this.price = price;
	}
	
	

	

}
